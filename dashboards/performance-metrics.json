        "legend": {
          "calcs": ["last", "max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "histogram_quantile(0.50, sum(rate(voiceassist_db_query_duration_seconds_bucket[5m])) by (le, query_type))",
          "legendFormat": "P50 - {{query_type}}",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "histogram_quantile(0.95, sum(rate(voiceassist_db_query_duration_seconds_bucket[5m])) by (le, query_type))",
          "legendFormat": "P95 - {{query_type}}",
          "refId": "B"
