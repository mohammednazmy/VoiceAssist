          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "histogram_quantile(0.99, sum(rate(voiceassist_db_query_duration_seconds_bucket[5m])) by (le, query_type))",
          "legendFormat": "P99 - {{query_type}}",
          "refId": "C"
        }
      ],
      "title": "Database Query Performance (P50, P95, P99)",
      "type": "timeseries"
    },
